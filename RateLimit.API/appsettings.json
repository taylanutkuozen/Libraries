{
  "IpRateLimiting": {
    "EnableEndPoint": true,
    "StackBlockedRequests": false,
    "HttpStatusCode" : 429,
    "IpWhitelist": [ "127.0.0.1", "::1" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "20s",
        "Limit": 3
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}